export PATH=/usr/local/mips-gcc-4.8.1/bin:$PATH

./configure --host=mips-elf --exec-prefix=/usr/local/mips-gcc-4.8.1 \
--with-newlib --enable-multilib \
--disable-newlib-io-c99-formats --enable-newlib-supplied-syscalls \
--enable-target-optspace \
--disable-option-checking \
--enable-newlib-nano-malloc --enable-newlib-nano-formatted-io --enable-newlib-reent-small

make clean
make && cp -f libc.a ./../../../../libc/platform/pic32mz/lib/ && cp -f libm.a ./../../../../libc/platform/pic32mz/lib/ && cp -f libg.a ./../../../../libc/platform/pic32mz/lib/